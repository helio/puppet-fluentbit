# Managed by puppet
<% @variables.each do |key, value| -%>
@SET <%= key %>=<%= value %>
<% end -%>

[SERVICE]
    Flush                     <%= @flush %>
    Daemon                    <%= @daemon %>
<% if @log_file %>
    Log_File                  <%= @log_file %>
<% end -%>
    Log_Level                 <%= @log_level %>
    Parsers_File              <%= @parsers_file %>
    Plugins_File               <%= @plugins_file %>
    Streams_file              <%= @streams_file %>
    HTTP_Server               <%= @http_server %>
    HTTP_Listen               <%= @http_listen %>
    HTTP_Port                 <%= @http_port %>
    Coro_Stack_Size           <%= @coro_stack_size %>
    storage.checksum          <%= @storage_checksum %>
<% if @storage_path %>
    storage.path              <%= @storage_path %>
<% end -%>
<% if @storage_sync %>
    storage.sync              <%= @storage_sync %>
<% end -%>
<% if @storage_backlog_mem_limit %>
    storage.backlog.mem_limit <%= @storage_backlog_mem_limit %>
<% end -%>

@INCLUDE input_*.conf
@INCLUDE output_*.conf
@INCLUDE filter_*.conf
