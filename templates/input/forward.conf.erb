# Managed by puppet
[INPUT]
    Name     forward
<%- if @listen -%>
    Listen   <%= @listen %>
<% end -%>
<% if @port -%>
    Port      <%= @port %>
<% end -%>
<% if @buffer_max_size -%>
    Buffer_Max_Size   <%= @buffer_max_size %>
<% end -%>
<% if @buffer_chunk_size -%>
    Buffer_Chunk_Size <%= @buffer_chunk_size %>
<% end -%>
